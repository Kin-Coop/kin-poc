<?xml version="1.0"?>
<extension key="kincopy" type="module">
  <file>kincopy</file>
  <name>kin copy</name>
  <description>Manage and create recurring offline contributions</description>
  <license>None</license>
  <maintainer>
    <author>Ben Mango</author>
    <email>ben@benmango.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Extension Repo">https://lab.civicrm.org/extensions/api4example</url>
    <url desc="Licensing"></url>
  </urls>
  <releaseDate>2025-04-22</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.40</ver>
  </compatibility>
  <comments>A custom module for Kin</comments>
  <civix>
    <namespace>CRM/Kincopy</namespace>
    <format>25.01.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
</extension>
