<?xml version="1.0"?>
<extension key="kincoop" type="module">
  <file>kincoop</file>
  <name>kincoop</name>
  <description>Kin Cooperative CiviCRM extension</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>James Ferrier</name>
      <email>rjferrier@protonmail.com</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="GitHub Page">https://github.com/Kin-Coop/kin-civicrm-ext</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-12-10</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.79</ver>
  </compatibility>
  <comments>Please refer to the README/GitHub page for details about this extension.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Kincoop</namespace>
    <format>25.10.2</format>
    <angularModule>crmKincoop</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@2.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Kincoop\AutomaticUpgrader</upgrader>
</extension>
